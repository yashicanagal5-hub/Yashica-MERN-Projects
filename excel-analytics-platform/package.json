{
  "name": "yashica-excel-analytics-platform",
  "version": "1.0.0",
  "description": "A powerful Yashica -Excel Analytics Platform with MERN stack-Zidio Developmemt Internship Project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "validate": "npm run lint && npm run test",
    "install:all": "npm install && cd client && npm install",
    "clean": "rm -rf node_modules client/node_modules && npm install",
    "docker:build": "docker build -t excel-analytics .",
    "docker:run": "docker run -p 5000:5000 excel-analytics",
    "seed": "node scripts/seedDatabase.js",
    "migrate": "node scripts/migrateDatabase.js",
    "backup": "node scripts/backupDatabase.js",
    "restore": "node scripts/restoreDatabase.js"
  },
  "keywords": [
    "excel",
    "analytics",
    "data-visualization",
    "charts",
    "mern-stack",
    "react",
    "nodejs",
    "mongodb",
    "express",
    "data-analysis",
    "spreadsheet",
    "xlsx",
    "statistics",
    "dashboard",
    "web-app"
  ],
  "author": "Yashica Nagal",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.23.24",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-cron": "^3.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/excel-analytics-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/excel-analytics-platform/issues"
  },
  "homepage": "https://github.com/your-username/excel-analytics-platform#readme",
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "uploads/*",
      "logs/*",
      "tests/fixtures/*"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/client/**",
      "!**/coverage/**",
      "!**/uploads/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    },
    "extends": [
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 2
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}